<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.elasticspring.core.region.RegionPostProcessorTest$SimpleObjectHolder">
		<constructor-arg>
			<bean class="org.elasticspring.core.region.RegionPostProcessorTest$SimpleWebserviceClient" />
		</constructor-arg>
	</bean>

	<bean class="org.elasticspring.core.region.RegionPostProcessor">
		<constructor-arg ref="regionProvider" />
	</bean>

	<bean id="regionProvider" class="org.elasticspring.core.region.StaticRegionProvider">
		<constructor-arg value="SA_EAST_1" />
	</bean>
</beans>